<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.thiagohe.todasasview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ViewTextView" />
        <activity android:name=".ViewButton" />
        <activity android:name=".ViewCheckBox" />
        <activity android:name=".ViewRadioButton" />
        <activity android:name=".ViewSpinner" />
        <activity android:name=".ViewImage" />
        <activity android:name=".ViewRecycler"></activity>
    </application>

</manifest>